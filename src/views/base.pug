doctype html
html(lang="ko")
    head
        title #{pagetitle} | Wetube
        link(link rel="stylesheet" href="https://unpkg.com/mvp.css")
    body
        header 
            h1 #{pageTitle}
            nav 
                ul 
                    li
                      a(href="/") 🏠Home
                    if loggedIn
                        li
                          a(href="users/logout") 로그아웃
                        li
                          a(href="/my-profile") #{loggedInUser.name}의 Profile
                    else
                        li
                          a(href="/join") 회원가입
                        li
                          a(href="/login") 로그인
                    li
                      a(href="videos/search") 🪄검색
                    li
                      a(href="videos/upload") Upload video
                    li
                      a(href="/memo") 📒게시판


        main
            block content
            include ./partials/footer
